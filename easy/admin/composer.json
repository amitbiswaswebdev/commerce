{
    "name": "easy/admin",
    "description": "E-commerce admin",
    "keywords": ["laravel", "e-commerce"],
    "license": "MIT",
    "authors": [
        {
            "name": "Amit Biswas",
            "email": "amit.biswas.webdev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "easy/theme": "*",
        "easy/customer": "*"
    },
    "autoload": {
        "psr-4": {
            "Easy\\Admin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Easy\\Admin\\AdminServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
