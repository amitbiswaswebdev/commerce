{
    "name": "easy/product",
    "description": "E-commerce product",
    "keywords": ["laravel", "e-commerce"],
    "license": "MIT",
    "authors": [
        {
            "name": "amitbiswaswebdev",
            "email": "amit.biswas.webdev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/filesystem": "^8.42",
        "illuminate/support": "^8.42",
        "illuminate/validation": "^8.42"
    },
    "autoload": {
        "psr-4": {
            "Easy\\Product\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Easy\\Product\\ProductServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
